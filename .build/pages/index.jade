                        input(type="file",name="file")
                      .input-addon
                        i.fa.fa-pencil
                        textarea(name="description",placeholder="Descreva a Ação",rows="8")
                    
                    .column
                      fieldset
                        legend
                          h4
                            i.fa.fa-key
